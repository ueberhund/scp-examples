{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Deny",
      "Action": "ec2:RunInstances",
      "Resource": "*",
      "Condition": {
        "ForAnyValue:ArnNotEquals": {
          "ec2:Subnet": [
            "arn:aws:ec2:*:*:subnet/subnet-*"
          ]
        }
      }
    }
  ]
}
